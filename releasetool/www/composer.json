{
	"minimum-stability": "stable",
	"license": "proprietary",
	"require": {
		"twig/twig": "1.6.5",
		"erusev/parsedown-extra": "0.7.x",
		"fab1en/rocket-chat-rest-client": "dev-master",
		"php": "^7.4 || ^8.0",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"guzzlehttp/guzzle": "^7.3",
		"guzzlehttp/psr7": "^1.7 || ^2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.0 || ^9.0",
		"friendsofphp/php-cs-fixer": "^3.5"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/tschreindl/rocket-chat-rest-client"
		}
	],

    "autoload": {
		"psr-4": { "OpenAPI\\Client\\" : "lib/YouTrackApilib/" }
	},
	"autoload-dev": {
		"psr-4": { "OpenAPI\\Client\\Test\\" : "lib/YouTrackApi/test/" }
	}
}
